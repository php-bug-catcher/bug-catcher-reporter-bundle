{
    "name": "php-sentinel/bug-catcher-reporter-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "description": "A Symfony bundle to report exceptions to Bug Catcher",
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.1",
        "kregel/exception-probe": "^1.0",
        "monolog/monolog": "^1.22 || ^2.0",
        "symfony/dependency-injection": "^4.4",
        "symfony/framework-bundle": "^4.4",
        "symfony/monolog-bundle": "v3.8.0"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "BugCatcher\\Reporter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BugCatcher\\Reporter\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/dotenv": "^4.4",
        "symfony/http-client": "^4.4",
        "symfony/orm-pack": "^2.4",
        "symfony/phpunit-bridge": "^4.4",
        "symfony/yaml": "^4.4"
    }
}
